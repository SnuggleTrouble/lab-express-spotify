<div>
  {{#each artistAlbums}}
    <div>
      <img src="{{this.images.0.url}}" alt="album-cover" />
      <h2>{{this.name}}</h2>
      <p>
        Release date:
        {{this.release_date}}
      </p>
      <p>
        Total tracks:
        {{this.total_tracks}}
      </p>
      <a href="/albums/tracks/{{this.id}}">View tracks</a>
    </div>
  {{/each}}
</div>